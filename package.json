    "express-session": "latest",
    "forever": "^0.15.3",
    "http-errors": "~1.7.1",
    "morgan": "^1.9.1",
    "pug": "2.0.3",
    "sprintf-js": "latest",
    "underscore.string": "latest"
  },
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/express": "^4.16.0",
    "@types/http-errors": "^1.6.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.15",
    "@types/sprintf-js": "^1.1.1",
    "ts-loader": "^5.3.1",
