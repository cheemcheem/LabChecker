{
  "name": "labchecker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "up": "NODE_ENV=production nolimit node_modules/forever/bin/forever start ./built/bin/www.js $UID",
    "compile": "tsc && npm run build",
    "build": "webpack",
    "clean": "rm -r built/views || true",
    "copy": "cp -r views/ built/views/ && cp -r public/stylesheets built/public/stylesheets",
    "start": "npm run clean && npm run compile && npm run copy && npm run up"
  },
  "dependencies": {
    "@types/promise.prototype.finally": "^2.0.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-mysql-session": "latest",
    "express-session": "latest",
    "forever": "^0.15.3",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "sprintf-js": "latest",
    "underscore.string": "latest"
  },
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/express": "^4.16.0",
    "@types/http-errors": "^1.6.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.5.2",
    "@types/sprintf-js": "^1.1.0",
    "ts-loader": "^5.3.0",
    "typescript": "^2.9.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  }
}
